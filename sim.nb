 "runSimFunc[",
 StyleBox["inputs_",
  FontSlant->"Italic"],
 "]"
}], "Subsubsection",
 CellChangeTimes->{{3.7215612981922617`*^9, 3.721561300600329*^9}, {
   3.7222767018885603`*^9, 3.722276706846478*^9}, 
   3.7222775415235167`*^9},ExpressionUUID->"f6b17094-06cf-479e-bcba-\
87a4d03395f4"],

Cell[BoxData[
 RowBox[{
  RowBox[{"runSimFunc", "[", 
   RowBox[{
   "roulType_", ",", " ", "bankRoll_", ",", " ", "minBet_", ",", "maxBet_", 
    ",", " ", "initBet_", ",", " ", "gamePerTrial_", ",", " ", "trials_"}], 
   "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"trialHistory", "=", 
       RowBox[{"{", "}"}]}], ",", " ", 
      RowBox[{"finalBankRoll", "=", 
       RowBox[{"{", "}"}]}], ",", " ", 
      RowBox[{"gameHistory", "=", 
       RowBox[{"{", "}"}]}], ",", " ", 
      RowBox[{"trialNum", "=", "0"}], ",", "dynPlot", ",", 
      RowBox[{"plotLiveUpdt", "=", "False"}]}], "}"}], ",", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
     "Updates", " ", "values", " ", "from", " ", "function", " ", "into", " ",
       "global", " ", "settings"}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"gameSettings", "=", 
        RowBox[{"AssociationThread", "[", 
         RowBox[{
          RowBox[{"Keys", "[", "gameSettings", "]"}], "\[Rule]", " ", 
          RowBox[{"{", 
           RowBox[{
           "roulType", ",", " ", "bankRoll", ",", " ", "minBet", ",", " ", 
            "maxBet", ",", " ", "initBet"}], "}"}]}], "]"}]}], ";", " ", 
       RowBox[{"simulationSettings", "=", 
        RowBox[{"AssociationThread", "[", 
         RowBox[{
          RowBox[{"Keys", "[", "simulationSettings", "]"}], "\[Rule]", " ", 
          RowBox[{"{", 
           RowBox[{"gamePerTrial", ",", " ", "trials"}], "}"}]}], "]"}]}], 
       ";"}], ")"}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"Run", " ", "trials", " ", "without", " ", "Prints"}], " ", 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Table", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"singleGameHistory", "=", 
         RowBox[{"runTrial", "[", "]"}]}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "snarf", " ", "relevant", " ", "data", " ", "from", " ", "trial", 
          " ", "for", " ", "later", " ", "analysis"}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"finalBankRoll", "=", 
         RowBox[{"Append", "[", 
          RowBox[{"finalBankRoll", ",", " ", 
           RowBox[{"Last", "[", "singleGameHistory", "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"trialHistory", "=", 
         RowBox[{"Append", "[", 
          RowBox[{"trialHistory", ",", " ", "singleGameHistory"}], "]"}]}], 
        ";"}], "\[IndentingNewLine]", ",", 
       RowBox[{"simulationSettings", "[", "\"\<numOfTrials\>\"", "]"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"--", 
        RowBox[{"--", 
         RowBox[{"--", 
          RowBox[{"--", " ", "Calculate"}]}]}]}], " ", "some", " ", "stats"}],
       " ", "*)"}], " ", "\[IndentingNewLine]", 
     RowBox[{"meanBankRoll", "=", 
      RowBox[{"N", "[", 
       RowBox[{"Mean", "[", "finalBankRoll", "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"stdev", "=", 
      RowBox[{"N", "[", 
       RowBox[{"StandardDeviation", "[", "finalBankRoll", "]"}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"varian", "=", 
      RowBox[{"N", "[", 
       RowBox[{"Variance", "[", "finalBankRoll", "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"resultCI", " ", "=", " ", 
        RowBox[{
         RowBox[{"MeanCI", "[", 
          RowBox[{
           RowBox[{"finalBankRoll", "-", 
            RowBox[{"gameSettings", "[", "\"\<bankRoll\>\"", "]"}]}], ",", 
           " ", 
           RowBox[{"ConfidenceLevel", "\[Rule]", ".95"}]}], "]"}], "/", 
         RowBox[{
         "simulationSettings", "[", "\"\<gamesPerTrial\>\"", "]"}]}]}], ";"}],
       "*)"}], "\[IndentingNewLine]", 
     RowBox[{"expVal", "=", 
      RowBox[{"Round", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Mean", "[", 
          RowBox[{"finalBankRoll", "-", 
           RowBox[{"gameSettings", "[", "\"\<bankRoll\>\"", "]"}]}], "]"}], 
         "/", 
         RowBox[{"simulationSettings", "[", "\"\<gamesPerTrial\>\"", "]"}]}], 
        ",", "0.0001"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"stdevExpVal", "=", 
      RowBox[{"N", "[", 
       RowBox[{"StandardDeviation", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"finalBankRoll", "-", 
           RowBox[{"gameSettings", "[", "\"\<bankRoll\>\"", "]"}]}], ")"}], 
         "/", 
         RowBox[{"simulationSettings", "[", "\"\<gamesPerTrial\>\"", "]"}]}], 
        "]"}], "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"finalPlot", "=", " ", 
      RowBox[{"ListLinePlot", "[", 
       RowBox[{"trialHistory", ",", "\[IndentingNewLine]", 
        RowBox[{"GridLines", "\[Rule]", "Automatic"}], ",", 
        RowBox[{"PlotRange", "\[Rule]", "All"}], ",", 
        RowBox[{"PlotLabel", "\[Rule]", "\"\<Simulation Results\>\""}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"BaseStyle", "->", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"FontWeight", "->", "\"\<Bold\>\""}], ",", 
           RowBox[{"FontSize", "\[Rule]", "15"}]}], "}"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"Axes", "\[Rule]", "True"}], ",", 
        RowBox[{"Frame", "\[Rule]", "True"}], ",", 
        RowBox[{"FrameLabel", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{"\"\<Bet Number\>\"", ",", "\"\<Bankroll ($)\>\""}], 
          "}"}]}], ",", "\[IndentingNewLine]", 
        RowBox[{"ImageSize", "\[Rule]", "Large"}], ",", 
        RowBox[{"MaxPlotPoints", "\[Rule]", "100"}], ",", 
        RowBox[{"PerformanceGoal", "\[Rule]", "\"\<Speed\>\""}]}], "]"}]}], 
     ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"guessDist", "=", 
      RowBox[{"FindDistribution", "[", "finalBankRoll", "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"Row", "[", 
      RowBox[{"{", 
       RowBox[{"finalPlot", ",", "\[IndentingNewLine]", 
        RowBox[{"Column", "[", 
         RowBox[{"{", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Grid", "[", 
            RowBox[{
             RowBox[{"{", "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"{", 
                RowBox[{"\"\<Games per trial: \>\"", ",", " ", 
                 RowBox[{
                 "simulationSettings", "[", "\"\<gamesPerTrial\>\"", "]"}]}], 
                "}"}], ",", "\[IndentingNewLine]", 
               RowBox[{"{", 
                RowBox[{"\"\<Number of trials: \>\"", ",", " ", 
                 RowBox[{
                 "simulationSettings", "[", "\"\<numOfTrials\>\"", "]"}]}], 
                "}"}], ",", "\[IndentingNewLine]", 
               RowBox[{"(*", 
                RowBox[{
                 RowBox[{"{", 
                  RowBox[{"\"\<% of bankrupcies: \>\"", ",", " ", 
                   RowBox[{
                    RowBox[{"N", "[", 
                    RowBox[{
                    RowBox[{"Count", "[", 
                    RowBox[{"finalBankRoll", ",", "0"}], "]"}], "/", 
                    RowBox[{
                    "simulationSettings", "[", "\"\<numOfTrials\>\"", "]"}]}],
                     "]"}], "*", "100"}]}], "}"}], ","}], "*)"}], 
               "\[IndentingNewLine]", 
               RowBox[{"{", 
                RowBox[{"\"\<Average earnings: \>\"", ",", "  ", 
                 RowBox[{"N", "[", 
                  RowBox[{
                   RowBox[{"Mean", "[", "finalBankRoll", "]"}], "-", 
                   RowBox[{"gameSettings", "[", "\"\<bankRoll\>\"", "]"}]}], 
                  "]"}]}], "}"}], ",", "\[IndentingNewLine]", 
               RowBox[{"{", 
                RowBox[{"\"\<Average final bankroll: \>\"", ",", " ", 
                 RowBox[{"N", "[", 
                  RowBox[{"Mean", "[", "finalBankRoll", "]"}], "]"}]}], 
                "}"}]}], "\[IndentingNewLine]", 
              RowBox[{"(*", 
               RowBox[{
                RowBox[{"{", 
                 RowBox[{"\"\<Expected value : \>\"", ",", 
                  RowBox[{
                   RowBox[{"ToString", "[", "expVal", "]"}], "<>", 
                   "\"\< \[PlusMinus] \>\"", "<>", 
                   RowBox[{"ToString", "[", 
                    RowBox[{"Round", "[", 
                    RowBox[{
                    RowBox[{"(", " ", 
                    RowBox[{
