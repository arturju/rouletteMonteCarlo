Cell[BoxData[
 RowBox[{
  RowBox[{"simSpin", "[", 
   RowBox[{"roulType_", ",", "betName_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"denom", "=", 
       RowBox[{"Switch", "[", 
        RowBox[{
        "roulType", ",", "\"\<enRoul\>\"", ",", "37", ",", "\"\<usRoul\>\"", 
         ",", "38", ",", "_", ",", 
         RowBox[{"Abort", "[", "]"}]}], "]"}]}], ",", "winOrLose", ",", 
      "mult", ",", "oneToOneDist", ",", "twoToOneDist", ",", "straightUp"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"oneToOneDist", "=", 
      RowBox[{"BinomialDistribution", "[", 
       RowBox[{"1", ",", 
        RowBox[{"18", "/", "denom"}]}], "]"}]}], ";", 
     RowBox[{"(*", " ", 
      RowBox[{
       RowBox[{"Bet", " ", "on", " ", "Red"}], ",", " ", "Black", ",", " ", 
       "Even", ",", " ", "Odd"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"twoToOneDist", "=", 
      RowBox[{"BinomialDistribution", "[", 
       RowBox[{"1", ",", 
        RowBox[{"12", "/", "denom"}]}], "]"}]}], ";", 
     RowBox[{"(*", " ", 
      RowBox[{"Bet", " ", "on", " ", "a", " ", "column"}], " ", "*)"}], " ", 
     "\[IndentingNewLine]", 
     RowBox[{"straightUp", "=", 
      RowBox[{"BinomialDistribution", "[", 
       RowBox[{"1", ",", 
        RowBox[{"1", "/", "denom"}]}], "]"}]}], ";", 
     RowBox[{"(*", " ", 
      RowBox[{"Bet", " ", "on", " ", "single", " ", "number"}], " ", "*)"}], 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"StringQ", "[", "betName", "]"}], ",", 
       RowBox[{"(*", " ", 
        RowBox[{
         RowBox[{
         "A", " ", "few", " ", "possibilities", " ", "of", " ", "bets"}], 
         "..."}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"StringMatchQ", "[", 
           RowBox[{"betName", ",", 
            RowBox[{"{", 
             RowBox[{
             "\"\<1to18\>\"", ",", "\"\<even\>\"", ",", "\"\<red\>\"", ",", 
              "\"\<black\>\"", ",", "\"\<odd\>\"", ",", "\"\<19to36\>\""}], 
             "}"}], ",", 
            RowBox[{"IgnoreCase", "\[Rule]", "True"}]}], "]"}], ",", " ", 
          RowBox[{
           RowBox[{"winOrLose", "=", 
            RowBox[{"RandomVariate", "[", "oneToOneDist", "]"}]}], ";", 
           RowBox[{"mult", "=", "1"}], ";"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"StringMatchQ", "[", 
           RowBox[{"betName", ",", 
            RowBox[{"{", 
             RowBox[{
             "\"\<1st12\>\"", ",", "\"\<2nd12\>\"", ",", "\"\<3rd12\>\"", 
              ",", "\"\<2to1\>\""}], "}"}], ",", 
            RowBox[{"IgnoreCase", "\[Rule]", "True"}]}], "]"}], ",", " ", 
          RowBox[{
           RowBox[{"winOrLose", "=", 
            RowBox[{"RandomVariate", "[", "twoToOneDist", "]"}]}], ";", 
           RowBox[{"mult", "=", "2"}], ";"}]}], "]"}], ";"}], 
       "\[IndentingNewLine]", ",", 
       RowBox[{"(*", " ", 
        RowBox[{"Else", ",", " ", 
         RowBox[{
         "bet", " ", "is", " ", "single", " ", "numbers", " ", "0", " ", "to",
           " ", "36"}]}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"winOrLose", "=", 
         RowBox[{"RandomVariate", "[", "straightUp", "]"}]}], ";", " ", 
        RowBox[{"mult", "=", "35"}], ";"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"winOrLose", "\[Equal]", "1"}], ",", "mult", ",", 
       RowBox[{"-", "1"}]}], "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellGroupingRules->{"GroupTogetherGrouping", 10000.},
